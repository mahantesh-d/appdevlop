pipeline{
    agent any
    stages{
        stage('Git Check Out'){
            steps{
                checkout scmGit(branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/mahantesh-d/appdevlop']])
                sh 'echo "Get pull completed"'
            }
        }
        stage('Build Docker image'){
            steps{
                sh 'sudo docker build -d --no-cache --pull --rm -f "DockerFile" -t pythonproject:latest "."'
                sh 'sudo docker ps -a'
            }
        }
    }
}